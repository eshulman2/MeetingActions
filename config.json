{
    "llm": "OpenAILike",
    "model": "gemini-2.0-flash",
    "verify_ssl": true,
    "additional_model_parameter": {
        "api_base": "https://someendpoint/v1beta/openai",
        "is_chat_model": true
    },
    "tools_config": {
        "jira_tool": {
            "server": "https://issues.redhat.com"
        }
    },
    "agent_config": {
        "max_iterations": 20,
        "verbose": true
    },
    "mcp_config": {
        "servers": [
            "http://127.0.0.1:8100/mcp"
        ]
    },
    "observability": {
        "enable": true,
        "secret_key": "******************************************",
        "public_key": "******************************************",
        "host": "http://localhost:3000"
    },
    "cache_config": {
        "enable": true,
        "ttl_hours": 1,
        "max_size_mb": 100,
        "host": "localhost",
        "port": 6380,
        "password": "12345"
    },
    "progressive_summarization": {
        "threshold_ratio": 0.75,
        "max_passes": 3,
        "strategy": "balanced",
        "chunk_threshold_ratio": 0.5,
        "chunk_size_ratio": 0.4,
        "chunk_overlap_tokens": 500
    }
}
